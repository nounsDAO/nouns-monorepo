    "@fortawesome/react-fontawesome": "0.2.2",
    "@heroicons/react": "^1.0.6",
    "@lingui/cli": "3.13.3",
    "@lingui/core": "3.13.3",
    "@lingui/detect-locale": "3.13.3",
    "@lingui/macro": "3.13.3",
    "@lingui/react": "3.13.3",
    "@netlify/functions": "^0.7.2",
    "@noundry/nouns-assets": "^1.5.0",
    "@nouns/sdk": "^0.4.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@usedapp/coingecko": "0.5.1",
    "@usedapp/core": "npm:usedapp-core-fork",
    "@walletconnect/ethereum-provider": "^2.8.4",
    "@walletconnect/modal": "^2.4.5",
    "@web3-react/core": "^6.1.9",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/ledger-connector": "^6.1.9",
    "@web3-react/trezor-connector": "^6.1.9",
    "@web3-react/walletlink-connector": "^6.2.14",
    "axios": "^0.30.0",
    "bad-words": "^3.0.4",
    "bignumber.js": "^9.3.0",
    "bootstrap": "^5.1",
    "bs-custom-file-input": "^1.3.4",
    "clsx": "^1.2.1",
    "connected-react-router": "^6.9.3",
    "dayjs": "^1.10.7",
    "eslint-config-react-app": "^6.0.0",
    "ethers": "^5.8.0",
    "framer-motion": "^6.5.1",
    "graphql": "^15.10.1",
    "pngjs": "^6.0.0",
    "ramda": "^0.30.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.10.9",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.0.0",
    "react-number-format": "^5.4.4",
    "react-redux": "^7.2.9",
    "react-router-dom": "^5.3.4",
    "react-scripts": "5.0.1",
    "react-stepz": "^1.0.2",
    "react-swipeable": "^7.0.0",
    "react-tooltip": "^4.2.21",
    "react-transition-group": "^4.4.5",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "remark-breaks": "^3.0.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@craco/craco": "^7.1.0",
    "assert": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "update-browserslist-db": "^1.0.13",
    "vm-browserify": "^1.1.2",
    "@types/bad-words": "^3.0.1",
    "@types/jest": "^26.0.15",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^12.20.55",
    "@types/pngjs": "^6.0.1",
    "@types/ramda": "^0.27.42",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "dotenv": "^10.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "lingui": {
    "catalogs": [
      {
        "path": "<rootDir>/src/locales/{locale}",
        "include": [
          "<rootDir>/src"
        ]
      }
    ],
    "compileNamespace": "cjs",
    "fallbackLocales": {
      "default": "en-US",
      "pseudo": "en-US"
    },
    "format": "po",
    "formatOptions": {
      "lineNumbers": false
    },
    "locales": [
      "en-US",
      "ja-JP",
      "zh-CN",
      "pseudo"
    ],
    "orderBy": "messageId",
    "rootDir": ".",
    "runtimeConfigModule": [
      "@lingui/core",
      "i18n"
    ],
    "sourceLocale": "en-US",
    "pseudoLocale": "pseudo"
  }
}