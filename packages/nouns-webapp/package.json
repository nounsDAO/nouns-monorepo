{
  "name": "@nouns/webapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "is-ci && exit 0 || (wagmi generate && graphql-codegen --config codegen.config.ts)",
    "build": "tsc -b && vite build",
    "clean": "rimraf dist .netlify",
    "graphql-codegen": "graphql-codegen --config codegen.config.ts",
    "graphql-codegen:watch": "graphql-codegen --config codegen.config.ts --watch",
    "dev": "vite",
    "i18n:compile": "lingui compile",
    "i18n:extract": "lingui extract",
    "i18n:pseudo": "lingui extract --locale pseudo && lingui compile",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
