{
  "name": "@nouns/sdk",
  "version": "1.0.0",
  "description": "Useful tooling for interacting with the Nouns protocol",
  "homepage": "https://github.com/nounsDAO/nouns-monorepo#readme",
  "repository": "github:nounsDAO/nouns-monorepo",
  "license": "GPL-3.0",
  "author": "Nounders",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react/*": {
      "import": "./dist/react/*.mjs",
      "require": "./dist/react/*.js"
    },
    "./*": {
      "import": "./dist/actions/*.mjs",
      "require": "./dist/actions/*.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist/**",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup && tsx scripts/deduplicate-abis.ts 'dist/actions/*.d.{ts,mts}' 'dist/react/*.d.{ts,mts}'",
    "clean": "rimraf dist",
    "codegen": "wagmi generate",
    "lint": "eslint",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/pngjs": "^6.0.1",
    "@wagmi/cli": "catalog:",
    "@wagmi/core": "catalog:",
    "dotenv": "^16.5.0",
    "glob": "^11.0.2",
    "pngjs": "^6.0.0",
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "vitest": "^3.2.4",
    "wagmi": "catalog:"
  },
  "peerDependencies": {
    "@wagmi/core": "^2.0.0",
    "wagmi": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "wagmi": {
      "optional": true
    }
  }
}
